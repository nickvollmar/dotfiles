[alias]
    lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
    com = !gitcom.sh
    co = checkout
    cb = checkout -b
    cp = cherry-pick
    poh = push origin HEAD
    st = status
    alt = !gitalt.sh
    riom = rebase -i origin/HEAD
    rhom = reset --hard origin/HEAD
    rs = restore --staged
    fixup = !git add -u && git commit --amend --no-edit && git push --force origin HEAD
